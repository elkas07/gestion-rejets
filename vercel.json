{
  "version": 2,
  "cleanUrls": true,
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate, proxy-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" },
        { "key": "X-SGR-Build", "value": "5.0.0-EMERALD-FINAL" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "routes": [
    { "src": "/index.tsx", "dest": "/index.tsx" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
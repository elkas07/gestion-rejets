{
  "version": 2,
  "cleanUrls": true,
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    }
  ],
  "routes": [
    { "src": "/index.tsx", "dest": "/index.tsx" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}